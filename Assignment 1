!pip install wandb
import numpy as np
import wandb
from keras.datasets import fashion_mnist
wandb.login()
wandb.init(project="assignment 1.1", entity="cs21m009",name='DL 1.1')
(x_train,y_train),(x_test,y_test)=fashion_mnist.load_data()
class_names = ['T-shirt/top','Trouser','Pullover','Dress','Coat','Sandal','Shirt','Sneaker','Bag','Ankle','boot']
set_images=[]
set_labels=[]
for i in range(len(x_train)):
    if len(set_labels)==10:
        break
    if class_names[y_train[i]] not in set_labels:
        set_images.append(x_train[i])
        set_labels.append(class_names[y_train[i]])
wandb.log({"Images ":[wandb.Image(img, caption=caption) for img,caption in zip(set_images,set_labels)]})
#print(len(set_images))
#print(len(set_labels))
